

<div class="well well-lg" id="map">
    @{
        ViewData["Title"] = "Contact";
    }

    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Find Us! <br /><small id="sm">Come and see our largest and most affordable designer collections!</small></h1>

                    <i class="fa fa-arrow-left fa-2x" aria-hidden="true"></i>
                    <p>@Html.ActionLink("Home", "Index", "Home")</p>

                <div class="col-lg-11" id="map">

                    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false">

                    </script>
                    <div style="overflow:hidden auto;height:500px;width:1020px;">
                        <div id="gmap_canvas" style="height:500px;width:1020px;">
                            <style>
                                #gmap_canvas_img {
                                    max-width: none !important;
                                    background: none !important;
                                }
                            </style>
                            <a class="google-map-code" href="http://www.themecircle.net/wordpress-portfolio/" id="get-map-data">themecircle</a>
                        </div>
                    </div>
                    <script type="text/javascript"> function init_map() { var myOptions = { zoom: 14, center: new google.maps.LatLng(33.00100175307313, -80.03923868717192), mapTypeId: google.maps.MapTypeId.ROADMAP }; map = new google.maps.Map(document.getElementById("gmap_canvas"), myOptions); marker = new google.maps.Marker({ map: map, position: new google.maps.LatLng(33.00100175307313, -80.03923868717192) }); infowindow = new google.maps.InfoWindow({ content: "<b>R&amp;D fashion and bridal</b><br/>119 N goose creek blvd <br/>29445 goose creek" }); google.maps.event.addListener(marker, "click", function () { infowindow.open(map, marker); }); infowindow.open(map, marker); } google.maps.event.addDomListener(window, 'load', init_map);
                    </script>
                    <script async defer
                    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB703ylB3v_cISBMStsPyx0Yl7BQFkLog0&callback=initMap">
                    </script>
                </div>
            </div>
        </div>


        <div id="contact" class="col-md-11">
            <div class="col-md-8" id="address">
                <div class="container-fluid">
                    <h2>Set up an Appointment!</h2>
                    <h4> We will help make your special day perfect!</h4>
                    <p><i class="fa fa-phone fa-3x" aria-hidden="true"></i>  (843)797-2032</p>
                    <p>or</p>
                    <p><i class="fa fa-envelope-o fa-3x"></i> <a href="mailto:feedback@startbootstrap.com">feedback@startbootstrap.com</a></p>
                </div>
            </div>

            <div class="col-md-6" id="address">
                <h3>R&D Fashion and Bridal</h3>
                <p>
                    Located in the Village Center<br />
                    119 Suite L North Hwy. 52<br />
                    Goose Creek, SC 29445<br />
                </p>

           <p><i class="fa fa-clock-o fa-3x" aria-hidden="true" alt="Operating Hours"></i><h4> Monday - Saturday: 10:00 AM to 7:00 PM</h4> <br /><h4>Close on Sundays</h4></p>
            </div>
        </div>
    </div>
</div>